<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".SeptemberProjectMainActivity"
    android:id="@+id/portrait_frag">


    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Make:"
        android:id="@+id/lbl_make"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="30dp"
        android:layout_marginTop="52dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/txt_make"
        android:layout_alignTop="@+id/lbl_make"
        android:layout_toRightOf="@+id/lbl_make"
        android:layout_toEndOf="@+id/lbl_make"
        android:layout_marginLeft="21dp"
        android:layout_marginStart="21dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Model:"
        android:id="@+id/lbl_model"
        android:layout_below="@+id/lbl_make"
        android:layout_alignLeft="@+id/lbl_make"
        android:layout_alignStart="@+id/lbl_make"
        android:layout_marginTop="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/txt_model"
        android:layout_alignTop="@+id/lbl_model"
        android:layout_alignLeft="@+id/txt_make"
        android:layout_alignStart="@+id/txt_make" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="HP:"
        android:id="@+id/lbl_hp"
        android:layout_below="@+id/lbl_model"
        android:layout_alignLeft="@+id/lbl_model"
        android:layout_alignStart="@+id/lbl_model"
        android:layout_marginTop="28dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/txt_hp"
        android:layout_alignBottom="@+id/lbl_hp"
        android:layout_alignLeft="@+id/txt_model"
        android:layout_alignStart="@+id/txt_model" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="0-60:"
        android:id="@+id/lbl_zero_sixty"
        android:layout_below="@+id/lbl_hp"
        android:layout_alignLeft="@+id/lbl_hp"
        android:layout_alignStart="@+id/lbl_hp"
        android:layout_marginTop="25dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/txt_zero_sixty"
        android:layout_alignBottom="@+id/lbl_zero_sixty"
        android:layout_alignLeft="@+id/txt_hp"
        android:layout_alignStart="@+id/txt_hp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Price:"
        android:id="@+id/lbl_price"
        android:layout_centerVertical="true"
        android:layout_alignLeft="@+id/lbl_zero_sixty"
        android:layout_alignStart="@+id/lbl_zero_sixty" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Medium Text"
        android:id="@+id/txt_price"
        android:layout_alignTop="@+id/lbl_price"
        android:layout_alignLeft="@+id/txt_zero_sixty"
        android:layout_alignStart="@+id/txt_zero_sixty" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageViewCar"
        android:layout_below="@+id/txt_price"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="26dp" />

</RelativeLayout>